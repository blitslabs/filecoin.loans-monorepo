extends layouts/_layout
block content
	.page7.page7_messages.js-page7
		+header7
		.page7__wrapper
			.page7__sidebar.js-page7-sidebar
				.page7__col
					button.page7__burger.js-page7-burger
						+i('burger')
					.page7__wrap
						.page7__teams
							a.page7__team.active(href="#")
								img.page7__pic(src="img/team-pic-1.png" alt="")
								.page7__counter 3
							a.page7__team(href="#")
								img.page7__pic(src="img/team-pic-2.png" alt="")
							a.page7__team.page7__team_add(href="#")
								+i('plus')
					+switchTheme('switch_theme', 'js-switch-theme')
				.page7__col
					.page7__head Sebo Studio Team
					.page7__wrap
						.page7__group
							.page7__category Started
							.page7__menu
								a.page7__link(href="#") # Welcome
								a.page7__link(href="#") # What’s New
						.page7__group
							.page7__category Channels
							.page7__menu
								a.page7__link(href="#")  # Announcements
								a.page7__link.active(href="#") # Sales
									.page7__counter 3
								a.page7__link(href="#") # Marketing
								a.page7__link(href="#") # Accounting
								a.page7__link(href="#") # Support
			.page7__inner
				.page7__top
					.page7__box
						.page7__title.title # Sales
						.page7__info Discussion room about our sales
					.page7__actions
						a.page7__action.page7__action_profile.js-page7-open(href="#")
							+i('profile')
							| 15
						a.page7__action(href="#")
							+i('favorite')
						a.page7__action(href="#")
							+i('settings')
				.comments
