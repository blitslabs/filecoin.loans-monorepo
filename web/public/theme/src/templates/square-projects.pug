---
title: Square Projects
---
extends layouts/_layout
block content
	.page
		+header('', true, 'Projects')
		.page__wrapper
			+sidebar('', '', 'active')
			.page__container
				.page__content
					.projects.js-projects
						.projects
							.sorting.js-sorting
								h1.sorting__title.title Projects
								.sorting__variants
									.sorting__text Show:
									select.sorting__select
										option(selected) All Projects
										option All Schedule
								.sorting__options
									+dropdown('Due Date')
									a.sorting__filters(href="#")
										+i('filters')
										+i('filters-1')
										| Filter
									.sorting__view
										a.sorting__box.active.js-sorting-box(href="#" data-class="cell")
											+i('sorting-1')
										a.sorting__box.js-sorting-box(href="#" data-class="line")
											+i('sorting-2')
										//- a.sorting__box.js-sorting-box(href="#")
										//- 	+i('sorting-3')
							.projects__container.cell.js-projects-container
								.projects__section
									.projects__inner
										.projects__head
											.projects__category Pending
											+sectionOptions
										.projects__group
											.card
												.card__head
													a.card__title.js-popup-open(href="#popup-details" data-effect="mfp-zoom-in") Development - Circle Website
													.card__info Iconspace Team
												.card__parameters
													.card__parameter
														+i('add')
														| 13
													.card__parameter
														+i('check-circle')
														| 4/5
													.card__parameter.card__parameter_time
														+i('clock')
														| 14 days left
												.card__scale
													.card__percent 85%
													.card__line
														.card__progress(style="width: 85%;")
												.users
													.users__item
														img.users__pic(src="img/ava-3.png" alt="")
													.users__item(style="background-color: #50B5FF") AF
													.users__item.users__item_counter +3
												+cardOptions
